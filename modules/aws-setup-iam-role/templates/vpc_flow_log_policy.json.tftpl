{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "CreateAndManageVPCFlowLogs",
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "logs:DescribeLogGroups",
        "logs:DescribeLogStreams"
			],
			"Resource": [
				"arn:aws:logs:${region}:${account_id}:log-group/*"
			]
		}
	]
}